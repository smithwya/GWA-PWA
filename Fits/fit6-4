ChebyCoeffs("P", "BstarBstar", "s = 1", {-0.351704 \pm 200.000000, 1.076486 \pm 200.000000, -0.746386 \pm 200.000000})

AddPole("P", 111.380661 \pm 1.000000, {"BB","BBstar","BstarBstar"}, {-78.049898 \pm 200.000000, 285.976097 \pm 200.000000, 158.224777 \pm 200.000000})
