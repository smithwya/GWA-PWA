ChebyCoeffs("P", "BstarBstar", "s = 1", {0.533465 \pm 200.000000, -0.689962 \pm 200.000000, -0.120895 \pm 200.000000})

AddPole("P", 115.379150 \pm 1.000000, {"BB","BBstar","BstarBstar"}, {10.570354 \pm 200.000000, -398.464301 \pm 200.000000, -98.400372 \pm 200.000000})
