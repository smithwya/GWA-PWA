ChebyCoeffs("P", "BstarBstar", "s = 1", {-0.520505 \pm 200.000000, 0.659168 \pm 200.000000, 0.138656 \pm 200.000000})

AddPole("P", 118.270075 \pm 1.000000, {"BB","BBstar","BstarBstar"}, {480.336841 \pm 200.000000, -64.497983 \pm 200.000000, -26.266855 \pm 200.000000})
